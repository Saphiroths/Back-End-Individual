        "ASPNETCORE_ENVIRONMENT": "Development"
      },

      "IIS Express": {
        "commandName": "IISExpress",
        "launchBrowser": false,
        "launchUrl": "api/item/all",
        "environmentVariables": {
          "ASPNETCORE_ENVIRONMENT": "Development"
        }
      }


    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/weatherforecast",
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}